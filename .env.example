APP_NAME=ai-mail-assistant
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME="${APP_NAME}"
MAIL_ENCRYPTION=null

SMTP_HOST=smtp.example.com
#SMTP_PORT=587
SMTP_PORT=465
SMTP_USERNAME=your-username@example.com
SMTP_PASSWORD=your-password
SMTP_ENCRYPTION=tls

# SMTP Configuration for Info Account (smtp1)
SMTP1_MAIL_HOST=smtp.example.com
SMTP1_MAIL_PORT=465
SMTP1_MAIL_USERNAME=info@example.com
SMTP1_MAIL_PASSWORD=your-password
SMTP1_MAIL_ENCRYPTION=smtps
SMTP1_MAIL_FROM_ADDRESS=info@example.com
SMTP1_MAIL_FROM_NAME="Info Team"

# SMTP Configuration for Damian Account (smtp2) 
SMTP2_MAIL_HOST=smtp.example.com
SMTP2_MAIL_PORT=465
SMTP2_MAIL_USERNAME=user@example.com
SMTP2_MAIL_PASSWORD=your-password
SMTP2_MAIL_ENCRYPTION=smtps
SMTP2_MAIL_FROM_ADDRESS=user@example.com
SMTP2_MAIL_FROM_NAME="User Name"

# IMAP Configuration - Default Account
IMAP_DEFAULT_ACCOUNT=default
IMAP_HOST=imap.example.com
IMAP_PORT=993
IMAP_PROTOCOL=imap
IMAP_ENCRYPTION=ssl
IMAP_VALIDATE_CERT=true
IMAP_USERNAME=your-username@example.com
IMAP_PASSWORD=your-password
IMAP_AUTHENTICATION=

# IMAP Configuration - Info Account
IMAP_INFO_HOST=imap.example.com
IMAP_INFO_PORT=993
IMAP_INFO_ENCRYPTION=ssl
IMAP_INFO_VALIDATE_CERT=true
IMAP_INFO_USERNAME=info@example.com
IMAP_INFO_PASSWORD=your-password

# IMAP Configuration - Damian Account
IMAP_DAMIAN_HOST=imap.example.com
IMAP_DAMIAN_PORT=993
IMAP_DAMIAN_ENCRYPTION=ssl
IMAP_DAMIAN_VALIDATE_CERT=true
IMAP_DAMIAN_USERNAME=user@example.com
IMAP_DAMIAN_PASSWORD=your-password
IMAP_DAMIAN_MESSAGE_LIMIT=50

# ImapEngine Configuration (uses IMAP_ values as fallbacks)
IMAPENGINE_DEFAULT_ACCOUNT=default
IMAPENGINE_HOST=
IMAPENGINE_PORT=
IMAPENGINE_USERNAME=
IMAPENGINE_PASSWORD=
IMAPENGINE_ENCRYPTION=

IMAPENGINE_DAMIAN_HOST=
IMAPENGINE_DAMIAN_PORT=
IMAPENGINE_DAMIAN_USERNAME=
IMAPENGINE_DAMIAN_PASSWORD=
IMAPENGINE_DAMIAN_ENCRYPTION=

IMAPENGINE_INFO_HOST=
IMAPENGINE_INFO_PORT=
IMAPENGINE_INFO_USERNAME=
IMAPENGINE_INFO_PASSWORD=
IMAPENGINE_INFO_ENCRYPTION=

# Email Signatures
EMAIL_SIGNATURE_DEFAULT="Best regards,\nYour Name\nYour Company"
EMAIL_SIGNATURE_INFO="Best regards,\nInfo Team\nYour Company"
EMAIL_SIGNATURE_DAMIAN="Best regards,\nDamian\nYour Company"

AI_API_URL=https://api.openai.com/v1/chat/completions
AI_API_KEY=

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
